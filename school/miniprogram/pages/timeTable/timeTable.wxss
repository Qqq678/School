.table {
  border-bottom: 1px solid #000;
  width: 100%;
  height: 4000rpx;
}
.tr {
  width: 100%;
  height: 8.3%;
  display: flex;
  align-items: center;
  border-top: 1px solid #000;
}
.table-cell {
  height: 100%;
  width: 12.5%;
  display:flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-left: 1px solid #000;
  font-size: 30rpx;
}

.bg-w {
  background: snow;
}
.bg-g {
  background: #e6f3f9;
}
.th {
  color: #fff;
  font-size: 30rpx;
  background: #3366ff;
}
.th:first-of-type {
  font-size: 16rpx;
}
.border_right {
  border-right: 1px solid #000;
}
